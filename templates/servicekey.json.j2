{
  "type": "service_account",
  "project_id": "{{ gcloud_key | from_json | json_query('project_id') }}",
  "private_key_id": "{{ gcloud_key | from_json | json_query('private_key_id') }}",
  "private_key": "{{ gcloud_key | from_json | json_query('private_key') }}",
  "client_email": "{{ gcloud_key | from_json | json_query('client_email') }}",
  "client_id": "{{ gcloud_key | from_json | json_query('client_id') }}",
  "auth_uri": "https://accounts.google.com/o/oauth2/auth",
  "token_uri": "https://accounts.google.com/o/oauth2/token",
  "auth_provider_x509_cert_url": "https://www.googleapis.com/oauth2/v1/certs",
  "client_x509_cert_url": "https://www.googleapis.com/robot/v1/metadata/x509/{{ gcloud_key | from_json | json_query('client_email') | urlencode }}"
}